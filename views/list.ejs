
   <%- include("header") -%>
   <div class="box" id="heading">
        <h1><%= ListTitle %> </h1>
    </div>

<div class="box">

  <% for(let i=0; i<newlistitems.length; i++){ %>
    <div class="item">
        <input type="checkbox" >
        <p><%= newlistitems[i] %></p>
    </div>
        
   <% } %>

   <form class="item" action="/" method="post">
    <input type="text" name="n1" placeholder="New item" autocomplete="off">
    <button type="submit" name="list" value=<%= ListTitle %>>+</button>
</form>

</div>
<%- include("footer") -%>



    

